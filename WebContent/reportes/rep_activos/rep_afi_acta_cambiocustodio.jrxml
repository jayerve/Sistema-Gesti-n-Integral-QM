	<queryString>
		<![CDATA[select a.ide_afcus,a.ide_gtemp,fecha_entrega_afcus,numero_acta_afcus,custodio,activo_afcus,custodio_anterior,cod_barra_afcus,cedula_custodio, cedula_anterior,cargo_custodio,cargo_custodio_ant,detalle_geare,
detalle_afact,valor_unitario_afact,color_afact,nro_factura_afact,
 coalesce(marca_afact,'N/A') as marca_afact,
 coalesce(serie_afact,'N/A') as serie_afact,
 coalesce(modelo_afact,'N/A') as modelo_afact,
detalle_afnoa,detalle_afest,cod_anterior_afact
from (
select ide_afcus,ide_afact,b.ide_gtemp,fecha_entrega_afcus,numero_acta_afcus,activo_afcus,cod_barra_afcus,documento_identidad_gtemp as cedula_custodio,
apellido_paterno_gtemp||' '||(case when apellido_materno_gtemp is null then '' else  apellido_materno_gtemp end)||' '||
primer_nombre_gtemp||' '||(case when segundo_nombre_gtemp is null then '' else segundo_nombre_gtemp end) as custodio
from afi_custodio a, gen_empleados_departamento_par b, gth_empleado c
where a.ide_geedp = b.ide_geedp and b.ide_gtemp = c.ide_gtemp
) a, (
select ide_afact,detalle_afact,valor_unitario_afact,color_afact,nro_factura_afact,marca_afact,serie_afact,modelo_afact,detalle_afnoa,detalle_afest,cod_anterior_afact
from afi_activo a, afi_nombre_activo b,afi_estado c
where a.ide_afnoa = b.ide_afnoa and a.ide_afest = c.ide_afest
) b,
(
select ide_afcus,d.detalle_gecaf as cargo_custodio_ant,documento_identidad_gtemp as cedula_anterior,
apellido_paterno_gtemp||' '||(case when apellido_materno_gtemp is null then '' else  apellido_materno_gtemp end)||' '||
primer_nombre_gtemp||' '||(case when segundo_nombre_gtemp is null then '' else segundo_nombre_gtemp end) as custodio_anterior
from afi_custodio a, gen_empleados_departamento_par b, gth_empleado c,  gen_cargo_funcional d
where a.gen_ide_geedp = b.ide_geedp and b.ide_gtemp = c.ide_gtemp and b.ide_gecaf = d.ide_gecaf
) c,
(
select ide_gtemp,detalle_gecaf,detalle_geare from gen_empleados_departamento_par a
    left join gen_area b on a.ide_geare = b.ide_geare
    left join gen_cargo_funcional c on a.ide_gecaf = c.ide_gecaf
    where coalesce(a.activo_geedp,false) in (true,false) and ide_geedp in (select max(ide_geedp) as ide_geedp from gen_empleados_departamento_par group by ide_gtemp )
    order by ide_gtemp
) d
where a.ide_afact = b.ide_afact
and a.ide_afcus = c.ide_afcus
and a.ide_gtemp = d.ide_gtemp
and numero_acta_afcus= $P{pnum_acta}
order by fecha_entrega_afcus desc]]>
	</queryString>
