	<queryString>
		<![CDATA[select a.ide_comov,b.ide_codem,mov_fecha_comov,nro_comprobante_comov,c.nombre_tepro,c.ruc_tepro,haber_codem,detalle_comov,valor,
                    cue_codigo_cocac,cue_descripcion_cocac,debe_codem,haber_codem,devengado_prmen,ide_gelua,codigo_clasificador_prcla
                    from cont_movimiento a
                    left join cont_detalle_movimiento b on a.ide_comov = b.ide_comov
                    left join tes_proveedor c on b.ide_tepro = c.ide_tepro
                    left join tes_tipo_concepto d on b.ide_tetic = d.ide_tetic
                    left join (select ide_comov,sum(haber_codem) as valor from cont_detalle_movimiento where transferencia_codem = true group by ide_comov) e on a.ide_comov = e.ide_comov
                    left join (select a.ide_prcla,b.ide_codem,codigo_clasificador_prcla,descripcion_clasificador_prcla, devengado_prmen
                    from pre_anual a, pre_mensual b,pre_clasificador c where a.ide_pranu = b.ide_pranu and a.ide_prcla = c.ide_prcla ) f on b.ide_codem = f.ide_codem
                    left join cont_catalogo_cuenta g on b.ide_cocac = g.ide_cocac
                    where a.ide_comov = $P{p_ide_comov}
                    order by ide_gelua]]>
	</queryString>
