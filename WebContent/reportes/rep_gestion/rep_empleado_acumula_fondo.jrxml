	<queryString>
		<![CDATA[select
EDP.ACUMULA_FONDOS_GEEDP,
EMP.IDE_GTEMP,
EMP.DOCUMENTO_IDENTIDAD_GTEMP,
EMP.APELLIDO_PATERNO_GTEMP || '  ' ||
EMP.APELLIDO_MATERNO_GTEMP || '  ' ||
EMP.PRIMER_NOMBRE_GTEMP || '  ' ||
EMP.SEGUNDO_NOMBRE_GTEMP AS NOMBRES,
SUCU.IDE_SUCU,
SUCU.NOM_SUCU,
DEP.IDE_GEDEP,
DEP.DETALLE_GEDEP,
EMP.ACTIVO_GTEMP
from GTH_EMPLEADO EMP
left join GEN_EMPLEADOS_DEPARTAMENTO_PAR EDP  ON EMP.IDE_GTEMP=EDP.IDE_GTEMP
LEFT JOIN SIS_SUCURSAL SUCU ON SUCU.IDE_SUCU=EDP.IDE_SUCU
LEFT JOIN  GEN_DEPARTAMENTO DEP ON DEP.IDE_GEDEP=EDP.IDE_GEDEP
where EDP.IDE_GEEDP IN($P!{IDE_GTEMP})
ORDER BY EDP.ACUMULA_FONDOS_GEEDP ASC,NOMBRES ASC]]>
	</queryString>
